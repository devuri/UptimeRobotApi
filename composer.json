{
  "name": "montross50/uptime-robot-api",
  "type": "library",
  "description": "API wrapper for uptime robot",
  "keywords": [
    "montross50",
    "UptimeRobotApi"
  ],
  "homepage": "https://github.com/Montross50/UptimeRobotApi",
  "license": "MIT",
  "authors": [
    {
      "name": "Trent Schmidt",
      "email": "sch43228@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.9|~7.0",
    "guzzlehttp/psr7": "^1.2",
    "jane/open-api": "^1.2",
    "php-http/guzzle5-adapter": "^0.6.0",
    "php-http/message": "^1.2",
    "illuminate/support": "5.0.*|5.1.*|5.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0||~5.0",
    "squizlabs/php_codesniffer": "~2.3",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Montross50\\UptimeRobotApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Montross50\\UptimeRobotApi\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
